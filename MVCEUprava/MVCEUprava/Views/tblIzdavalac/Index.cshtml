@model IEnumerable<MVCEUprava.Models.tblIzdavalac>

@{
    ViewBag.Title = "Index";
}

<br/>
<p>
    @Html.ActionLink("Kreiraj novog izdavaoca", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Naziv)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Naziv)
        </td>
        <td>
            @Html.ActionLink("Modifikuj", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Obriši", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
